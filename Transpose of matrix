void transpose(vector<vector<int> >& matrix, int n)
    {  
        for(int i=0;i<n;i++)
        {
            for(int j=i;j<n-1;j++)
            {
                if(i==j+1)
                continue;
                
                swap(matrix[i][j+1],matrix[j+1][i]);
            }
        }
    }
